{
   "name": "mongo-migration",
   "version": "0.0.1",
   "description": "mongo migrations",
   "type": "commonjs",
   "devDependencies": {
      "@types/node": "^16.11.10",
      "ts-node": "10.9.1",
      "typescript": "4.5.2"
   },
   "dependencies": {
      "migrate-mongo": "^11.0.0",
      "mongodb": "^6.11.0",
      "reflect-metadata": "^0.1.13"
   },
   "scripts": {
      "migration:run": "cd ./src && npx migrate-mongo up",
      "migration:revert": "cd ./src && npx migrate-mongo down",
      "seed:run": "cd ./src && node seeds/index.js",
      "seed:clear": "cd ./src && node seeds/index.js clear"
   }
}
